<% include partials/header %>

<h1>Index Page is welcoming you, master!</h1>

<% blogs.forEach((blog) => { %>
  <div>
    <h2> <%= blog.title %></h2>
    <img src="<%= blog.image %>" alt="<%= blog.title %>">
    <p><%= blog.body %></p>
    <p><%= blog.created %></p>
  </div>
<% }) %>

<% include partials/footer %>
<% include partials/header %>

<h1>Index Page is welcoming you, master!</h1>

<% blogs.forEach((blog) => { %>
  <div>
    <h2> <%= blog.title %></h2>
    <img src="<%= blog.image %>" alt="<%= blog.title %>">
    <p><%= blog.body.substring(0,100) %>...</p>
    <p><%= blog.created %></p>
    <a href="/blogs/<%= blog._id%>" class="ui violet inverted button">Read More</a>
  </div>
<% }) %>

<% include partials/footer %>